{
    "name": "Web Chat - Marina AI (da aggiungere al workflow esistente)",
    "nodes": [
        {
            "parameters": {
                "httpMethod": "POST",
                "path": "chat",
                "responseMode": "responseNode",
                "options": {
                    "allowedOrigins": "*"
                }
            },
            "id": "webhook-web-chat",
            "name": "Webhook Web (Sito)",
            "type": "n8n-nodes-base.webhook",
            "typeVersion": 2,
            "position": [
                0,
                0
            ],
            "webhookId": "chat"
        },
        {
            "parameters": {
                "mode": "manual",
                "duplicateItem": false,
                "assignments": {
                    "assignments": [
                        {
                            "id": "chatInput",
                            "name": "chatInput",
                            "value": "={{ $json.body.message }}",
                            "type": "string"
                        },
                        {
                            "id": "sessionId",
                            "name": "sessionId",
                            "value": "={{ $json.body.sessionId }}",
                            "type": "string"
                        }
                    ]
                },
                "options": {}
            },
            "id": "set-web-context",
            "name": "Set Web Context",
            "type": "n8n-nodes-base.set",
            "typeVersion": 3.4,
            "position": [
                220,
                0
            ]
        },
        {
            "parameters": {
                "respondWith": "json",
                "responseBody": "={{ { \"message\": $json.output } }}",
                "options": {}
            },
            "id": "respond-webhook",
            "name": "Respond to Webhook",
            "type": "n8n-nodes-base.respondToWebhook",
            "typeVersion": 1.1,
            "position": [
                880,
                0
            ]
        }
    ],
    "connections": {
        "Webhook Web (Sito)": {
            "main": [
                [
                    {
                        "node": "Set Web Context",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Set Web Context": {
            "main": [
                [
                    {
                        "node": "Marina AI Agent",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Marina AI Agent": {
            "main": [
                [
                    {
                        "node": "Respond to Webhook",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        }
    },
    "pinData": {},
    "settings": {
        "executionOrder": "v1"
    },
    "staticData": null,
    "tags": [],
    "triggerCount": 0,
    "versionId": "web-chat-v1"
}